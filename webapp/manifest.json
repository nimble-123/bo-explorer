{
	"_version": "1.8.0",
	"sap.app": {
		"id": "dev.bo-explorer",
		"type": "application",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "dev.bo-explorer.view.App",
			"type": "XML",
			"asyn": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.46.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.46.0"
				},
				"sap.m": {
					"minVersion": "1.46.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.46.0"
				},
				"sap.f": {
					"minVersion": "1.46.0"
				},
				"sap.uxap": {
					"minVersion": "1.46.0"
				},
				"sap.tnt": {
					"minVersion": "1.46.0"
				},
				"sap.ui.codeeditor": {
					"minVersion": "1.46.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "dev.bo-explorer.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "dev.bo-explorer.view",
				"controlId": "splitApp",
				"controlAggregation": "detailPages",
				"bypassed": {
					"target": ["welcome"]
				},
				"async": true,
				"clearTarget": false
			},
			"routes": [{
				"pattern": "",
				"name": "api",
				"target": ["master", "detailInitial"]
			}, {
				"pattern": "api/{id}/:entityType:/:entityId:",
				"name": "apiId",
				"target": ["master", "detail"]
			}],
			"targets": {
				"master": {
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master",
					"controlAggregation": "masterPages"
				},
				"detailInitial": {
					"viewName": "DetailInitial",
					"viewId": "detailInitial",
					"viewLevel": 2
				},
				"detail": {
					"viewName": "Detail",
					"viewId": "detail",
					"viewLevel": 2
				}
			}
		}
	}
}
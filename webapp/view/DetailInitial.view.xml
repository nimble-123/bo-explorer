							width="100%">
						<custom:TitleLink
								level="H1"
								titleStyle="H1"
								text=""
								href="#/api/sap.ui.model"/>
						<Text
								text=""
								class="sapUiSmallMarginBottom"/>
						<Label
								text=""/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow visible="{= !${versionData>/isOpenUI5}}">

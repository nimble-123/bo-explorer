							width="100%">
						<custom:TitleLink
								level="H1"
								titleStyle="H1"
								text=""
								href="#/api/"/>
						<Text
								text=""
								class="sapUiSmallMarginBottom"/>
						<Label
								text=""/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
			</l:BlockLayoutRow>

			<l:BlockLayoutRow>
				<l:BlockLayoutCell
						class="largeCell"
						backgroundColorSet="ColorSet11"
						backgroundColorShade="ShadeC">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								level="H1"
								titleStyle="H1"
								text=""
								href="#/api/"/>
						<Text
								text=""
								class="sapUiSmallMarginBottom"/>
						<Label
								text=""/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="largeCell"
						backgroundColorSet="ColorSet11"
						backgroundColorShade="ShadeB">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								level="H1"
								titleStyle="H1"
								text=""
								href="#/api/"/>
						<Text
								text=""
								class="sapUiSmallMarginBottom"/>
						<Label
								text=""/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>

				<l:BlockLayoutCell
						class="largeCell"
						backgroundColorSet="ColorSet11"
						backgroundColorShade="ShadeA"
						width="2">
					<l:VerticalLayout
							width="100%">
						<custom:TitleLink
								level="H1"
								titleStyle="H1"
								text=""
